<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Magenest_Movie::config_movie"
                                      title="Movie"/>
                            <resource id="Magenest_Movie::config_login"
                                      title="Social Login"/>
                        </resource>
                    </resource>
                </resource>
                <resource id="Magenest_Movie::movie"
                          title="Movie">
                    <resource id="Magenest_Movie::magenest_movie"
                              title="Movie"/>
                    <resource id="Magenest_Movie::magenest_director"
                              title="Director"/>
                    <resource id="Magenest_Movie::magenest_actor"
                              title="Actor"/>
                </resource>

            </resource>
        </resources>
    </acl>
</config>